{
  "$schema": "https://frontmatter.codes/frontmatter.schema.json",
  "frontMatter.custom.scripts": [
    {
      "id": "r2-upload",
      "title": "Upload to R2",
      "script": "./r2_upload.js",
      "command": "node",
      "type": "mediaFile"
    }
  ],
  "frontMatter.external.media.scripts": [
    "r2-upload"
  ],
  "frontMatter.taxonomy.contentTypes": [
    {
      "name": "default",
      "pageBundle": false,
      "previewPath": null,
      "fields": [
        {
          "title": "title",
          "name": "title",
          "type": "string"
        },
        {
          "title": "date",
          "name": "date",
          "type": "datetime"
        },
        {
          "title": "draft",
          "name": "draft",
          "type": "draft"
        },
        {
          "title": "summary",
          "name": "summary",
          "type": "string"
        },
        {
          "title": "categories",
          "name": "categories",
          "type": "categories"
        },
        {
          "title": "tags",
          "name": "tags",
          "type": "tags"
        }
      ]
    }
  ],
  "frontMatter.framework.id": "hugo",
  "frontMatter.content.publicFolder": "static",
  "frontMatter.preview.host": "http://localhost:1313",
  "frontMatter.content.pageFolders": [
    {
      "title": "posts",
      "path": "[[workspace]]/content/posts"
    }
  ],
  "frontMatter.git.enabled": true,
  "frontMatter.website.host": "https://aoiro-labo.com",
  "frontMatter.content.publicPath": "https://images.aoiro-labo.com",
  "frontMatter.media.storage.type": "external",
  "frontMatter.media.external.command": "node [[workspace]]/r2_upload.js \"[[filePath]]\" \"[[slug]]\"",
  "frontMatter.dashboard.openOnStart": true,
  "frontMatter.panel.openOnSupportedFile": true
}